class Solution(object):
    def countConsistentStrings(self, allowed, words):
        """
        :type allowed: str
        :type words: List[str]
        :rtype: int
        """
        count=0
        allowed_set=set(allowed)
        for word in words:
            # the intersection length should be same as the word length
            if len(set(word).intersection(allowed_set))==len(set(word)):
                count+=1
        return count
        
